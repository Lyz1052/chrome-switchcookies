{
    "name": "Cookieswitcher",
    "version": "0.1",
    "description": "switch cookies",
    "manifest_version": 2,
    "browser_action" :{
        "default_icon": "icon.png",
        "default_title": "Cookieswitcher",
        "default_popup": "popup.html"
    },
     "options_page": "settings.html",
    "permissions": [
        "cookies",
        "tabs",
        "http://*/*"
    ]
}